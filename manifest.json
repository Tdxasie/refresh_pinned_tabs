{
   "browser_action": {
      "default_icon": "icon.png"
   },
   "description": "Automatically pins Youtube, Facebook and Twitter tabs",
   "manifest_version": 2,
   "name": "Fav Tabs Pinner",
   "version": "1.0",
   "options_page": "options/options.html",
   "permissions": [
     "tabs",
     "storage"
   ],
   "background":{
     "scripts": ["background/background.js"]
   }
}
